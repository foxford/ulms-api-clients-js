{
  "env":{
    "test": {
      "plugins": ["@babel/plugin-transform-modules-commonjs"]
    },
    "es": {
      "presets": [
        ["@babel/preset-env", {
          "modules": false,
          "targets": "last 2 versions"
        }]
      ],
      "plugins": [
        "babel-plugin-annotate-pure-calls"
      ]
    },
    "lib": {
      "presets": ["@babel/preset-env"]
    },
    "rollup": {
      "presets": [
        ["@babel/preset-env", {
          "modules": false
        }]
      ]
    }
  }
}
